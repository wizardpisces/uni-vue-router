{
  "name": "uni-vue-router",
  "version": "2.0.4",
  "description": "uniapp router, vue like API",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/src/index.d.ts",
  "typings": "dist/types/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rimraf ./dist",
    "prebuild": "rimraf dist .rpt2_cache",
    "build": "rollup -c build/rollup.config.js",
    "release": "bash build/release.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/__tests__/*.*.ts": [
      "jest"
    ]
  },
  "engines": {
    "node": ">=8.x"
  },
  "keywords": [
    "uni-app",
    "router",
    "vue-router"
  ],
  "homepage": "https://github.com/wizardpisces/uni-vue-router.git",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:wizardpisces/uni-vue-router.git"
  },
  "author": "1275839779@qq.com",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/webpack-env": "^1.14.0",
    "husky": "^6.0.0",
    "jest": "^25.5.4",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "terser": "^4.1.3",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "lint-staged": "^10.5.4",
    "vue": "^2.6.10"
  }
}
